<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jlw.dao.GoodsDao">
    <select id="selectGood" resultType="com.jlw.domain.Goods">
        select id,name,amount,price from goods where id=#{goodId}
    </select>

</mapper>